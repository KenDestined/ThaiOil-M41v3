<aura:component
    implements="force:lightningQuickAction,flexipage:availableForAllPageTypes,force:hasRecordId,force:hasSObjectName"
    access="global" controller="CTRSapInformationController">

    <!-- <aura:attribute name="requestFormObj" type="CTRRequestFormItem__c" default="{}" /> -->

    <aura:attribute name="requestFormHeaderObj" type="CTRRequestFormHeader__c" default="{}" />
    <aura:attribute name="requestFormItemObj" type="CTRRequestFormItem__c" default="{}" />

    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="recordIdFormHeader" type="String" />
    <aura:attribute name="recordTypeNameCustomer" type="Boolean" />
    <aura:attribute name="recordTypeName" type="String" />
    <aura:attribute name="currentUserName" type="String" />
    <aura:attribute name="WhereCityCondition" type="String" />

    <aura:attribute name="isModalOpen" type="Boolean" default="false" />
    <aura:attribute name="showLoading" type="Boolean" default="false" />
    <aura:attribute name="LookupChanged" type="Boolean" default="false" />


    <aura:attribute name="validateMissingField" type="String" />
    <aura:attribute name="validateFieldShow" type="Boolean" default="false" />

    <aura:attribute name="CountryCodePhoneList" type="List" />
    <aura:attribute name="TypeOfBusinessList" type="List" />

    <aura:attribute name="DistributionChannelList" type="List" />
    <aura:attribute name="SalesOrgList" type="List" />
    <aura:attribute name="DivisionList" type="List" />
    <aura:attribute name="PurchasingOrgList" type="List" />
    <aura:attribute name="CashManagementGroupList" type="List" />
    <aura:attribute name="AccountGroupList" type="List" />
    <aura:attribute name="TradingPartnerList" type="List" />
    <aura:attribute name="RegionList" type="List" />


    <aura:attribute name="LanguageList" type="List" />

    <aura:attribute name="SalesDistrictList" type="List" />
    <aura:attribute name="SalesOfficeList" type="List" />
    <aura:attribute name="CurrencyList" type="List" />
    <aura:attribute name="DeliveryPriorityList" type="List" />
    <aura:attribute name="ShippingConditionsList" type="List" />
    <aura:attribute name="DeliveryPlantList" type="List" />
    <aura:attribute name="IncotermsList" type="List" />
    <aura:attribute name="Incoterms2List" type="List" />
    <aura:attribute name="PaymentTermList" type="List" />
    <aura:attribute name="RiskCategoryList" type="List" />

    <aura:attribute name="SalesGroupList" type="List" />
    <aura:attribute name="PartialDeliveriesperitemList" type="List" />
    <aura:attribute name="CreditControlAreaList" type="List" />
    <aura:attribute name="PaymentGuaranteeProcedureList" type="List" />
    <aura:attribute name="AccountAssignmentGroupList" type="List" />
    <aura:attribute name="TaxClassificationList" type="List" />

    <aura:attribute name="SentGeneralView" type="Boolean" />
    <aura:attribute name="PendingApproval" type="Boolean" />
    <aura:attribute name="isNotShipTo" type="Boolean" />
    <aura:attribute name="GeneralWarning" type="Boolean" />
    <aura:attribute name="SAPSyncDoNotChange" type="Boolean" />


    <aura:attribute name="ChangedFieldForEDITHeader" type="List" default="[]" />
    <aura:attribute name="ChangedFieldForEDITItem" type="List" default="[]" />
    <aura:handler name="OnChangeLookup" event="c:CTROnChangeLookupEvent" action="{!c.LookupChanged}" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <lightning:recordEditForm recordId="{!v.recordId}" objectApiName="CTRRequestFormItem__c" onload="{!c.handleLoad}">
        <lightning:messages />
        <div class="slds-modal__content slds-p-around_medium content-body">
            <div style="text-align: center;">

            </div>
            <div class="slds-var-m-top_medium slds-var-m-bottom_x-large" style="background-color: white;">

                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1" style="overflow-y: auto;">
                    <lightning:tabset selectedTabId="generalView">
                        <!-- Tab General View  -->
                        <lightning:tab label="General View" id="generalView">
                            <div class="slds-grid slds-wrap">
                                <aura:if isTrue="{!v.GeneralWarning}">
                                    <div class="slds-notify_container slds-is-relative">
                                        <div class="slds-notify slds-notify_toast slds-theme_warning" role="status">
                                            <span class="slds-assistive-text">warning</span>
                                            <span
                                                class="slds-icon_container slds-icon-utility-warning slds-m-right_small slds-no-flex slds-align-top"
                                                title="Description of icon when needed">
                                                <lightning:icon iconName="utility:warning" alternativeText="warning"
                                                    size="large" title="large size" />
                                            </span>
                                            <div class="slds-notify__content">
                                                <h2 class="slds-text-heading_small ">This information has been created
                                                    in General View on SAP. If you edit information, it will be update
                                                    instead of current information.</h2>
                                            </div>
                                            <div class="slds-notify__close">

                                            </div>
                                        </div>
                                    </div>
                                </aura:if>
                                <!-- Name Section -->
                                <div class="slds-col slds-size_1-of-1">
                                    <div class="slds-section slds-is-open" aura:id="nameSection">
                                        <h3 class="slds-section__title">
                                            <button type="button" aria-controls="nameDetail" aria-expanded="true"
                                                class="slds-button slds-section__title-action" data-auraid="nameSection"
                                                onclick="{!c.handleToggleSection}">
                                                <lightning:icon iconName="utility:switch" alternativeText="switch"
                                                    title="" size="x-small"
                                                    class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                                <span class="slds-truncate" title="Name">Company Name</span>
                                            </button>
                                        </h3>

                                        <div class="slds-section__content" id="nameDetail">
                                            <div class="slds-grid slds-wrap slds-var-p-around_large">
                                                <!-- Full Name * -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        <!-- <p style="color: red;">* &nbsp;</p> -->
                                                        Full Name
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6"
                                                    style="padding-bottom: 16px; text-align: start;">
                                                    {!v.requestFormHeaderObj.CompanyNameLocal__c}
                                                </div>
                                                <!-- Name * -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 50px; display: flex;">
                                                        <p style="color: red;">* &nbsp;</p>Name
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6"
                                                    style="padding-bottom: 16px; text-align: start;">
                                                    <lightning:input name="CustomerNameLocal1__c" class="help-txt-hidden"
                                                        value="{!v.requestFormHeaderObj.CustomerNameLocal1__c}"
                                                        id="Name1" disabled="{!v.PendingApproval}"
                                                        onchange="{!c.handleAddJsonHeader}" maxlength="35" />

                                                    <!--<lightning:inputField fieldName="CustomerNameLocal1__c"
                                                        variant="label-hidden" required="true"  
                                                        value="{!v.requestFormHeaderObj.CustomerNameLocal1__c}" />-->
                                                </div>
                                                <!-- Radio Data not correct -->
                                                <div class="slds-col slds-size_1-of-6">
                                                    <div class="slds-form-element__control" style="margin-top: 20px;
                                                        padding-left: 24px;">
                                                        <lightning:input type="checkbox" class="help-txt-hidden"
                                                            name="CustomerNameLocalDatanotcorrect__c"
                                                            label="Data not correct"
                                                            checked="{!v.requestFormHeaderObj.CustomerNameLocalDatanotcorrect__c}"
                                                            onchange="{!c.handleCheckboxChange}"
                                                            disabled="{!v.PendingApproval}"
                                                            id="v.requestFormHeaderObj.CustomerNameLocalDatanotcorrect__c" />
                                                    </div>
                                                </div>
                                                <!--  Name input 2  -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <p style="text-align: left; padding-left: 62px;">
                                                        <!-- {!v.requestFormHeaderObj.CompanyNameLocal__c} -->
                                                    </p>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6" style="padding-bottom: 16px;">
                                                    <lightning:input name="CustomerNameLocal2__c" class="help-txt-hidden"
                                                        value="{!v.requestFormHeaderObj.CustomerNameLocal2__c}"
                                                        id="Name2" disabled="{!v.PendingApproval}"
                                                        onchange="{!c.handleAddJsonHeader}" maxlength="35" />
                                                    <!--<lightning:inputField fieldName="CustomerNameLocal2__c"
                                                        variant="label-hidden"  value="{!v.requestFormHeaderObj.CustomerNameLocal2__c}" />-->
                                                </div>
                                                <!-- Full Name EN* -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-bottom_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        <!-- <p style="color: red;">* &nbsp;</p> -->
                                                        Full Name (EN)
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6"
                                                    style="padding-bottom: 16px; text-align: start;">
                                                    {!v.requestFormHeaderObj.CompanyNameENF__c}
                                                </div>
                                                <!-- Name(EN) * -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 50px; display: flex;">
                                                        <p style="color: red;">* &nbsp;</p>Name(EN)
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6"
                                                    style="padding-bottom: 16px; text-align: start;">
                                                    <lightning:input name="CustomerNameEN1__c" class="help-txt-hidden"
                                                        value="{!v.requestFormHeaderObj.CustomerNameEN1__c}"
                                                        id="Name1 Eng" disabled="{!v.PendingApproval}"
                                                        onchange="{!c.handleAddJsonHeader}" maxlength="35" />
                                                    <!--<lightning:inputField fieldName="CustomerNameEN1__c" variant="label-hidden" 
                                                        required="true"  value="{!v.requestFormHeaderObj.CustomerNameEN1__c}"/>-->
                                                </div>
                                                <!-- Radio Data not correct NameENG-->
                                                <div class="slds-col slds-size_1-of-6">
                                                    <div class="slds-form-element__control" style="margin-top: 20px;
                                                        padding-left: 24px;">
                                                        <lightning:input type="checkbox" name="options" class="help-txt-hidden"
                                                            label="Data not correct"
                                                            checked="{!v.requestFormHeaderObj.CustomerNameEN1Datanotcorrect__c}"
                                                            onchange="{!c.handleCheckboxChange}"
                                                            disabled="{!v.PendingApproval}"
                                                            id="v.requestFormHeaderObj.CustomerNameEN1Datanotcorrect__c" />
                                                    </div>
                                                </div>
                                                <!--  name(EN) input 2 -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <p style="text-align: left; padding-left: 62px;">
                                                        <!-- {!v.requestFormHeaderObj.CompanyNameENF__c} -->
                                                    </p>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6" style="padding-bottom: 16px;">
                                                    <lightning:input name="CustomerNameEN2__c" class="help-txt-hidden"
                                                        value="{!v.requestFormHeaderObj.CustomerNameEN2__c}"
                                                        id="Name2 Eng" disabled="{!v.PendingApproval}"
                                                        onchange="{!c.handleAddJsonHeader}" maxlength="35" />
                                                    <!--<lightning:inputField fieldName="CustomerNameEN2__c" variant="label-hidden" value="{!v.requestFormHeaderObj.CustomerNameEN2__c}"/>-->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Search Terms Section -->
                                <div class="slds-col slds-size_1-of-1">
                                    <div class="slds-section slds-is-open" aura:id="searchTermsSection">
                                        <h3 class="slds-section__title">
                                            <button type="button" aria-controls="searchTermsDetail" aria-expanded="true"
                                                class="slds-button slds-section__title-action"
                                                data-auraid="searchTermsSection" onclick="{!c.handleToggleSection}">
                                                <lightning:icon iconName="utility:switch" alternativeText="switch"
                                                    title="" size="x-small"
                                                    class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                                <span class="slds-truncate" title="Search Terms">Search Terms</span>
                                            </button>
                                        </h3>
                                        <div class="slds-section__content" id="searchTermsDetail">
                                            <div class="slds-grid slds-wrap slds-var-p-around_large">
                                                <!-- Search Term 1* -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 50px; display: flex;">
                                                        <p style="color: red;">* &nbsp;</p>Search Term 1
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6"
                                                    style="padding-bottom: 16px; text-align: start;">
                                                    <aura:if isTrue="{!v.recordTypeNameCustomer == True}">
                                                        <lightning:input name="CustomerSearchTermLocal__c" class="help-txt-hidden"
                                                            label="Search Term 1" variant="label-hidden"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            disabled="{!v.PendingApproval}"
                                                            value="{!v.requestFormHeaderObj.CustomerSearchTermLocal__c}"
                                                            maxlength="20" />
                                                        <!--<lightning:inputField
                                                            fieldName="CustomerSearchTermLocal__c"
                                                            variant="label-hidden" required="true"  value="{!v.requestFormHeaderObj.CustomerSearchTermLocal__c}" />-->
                                                    </aura:if>
                                                    <aura:if isTrue="{!v.recordTypeNameCustomer == False}">
                                                        <lightning:input name="SupplierSearchTermLocal__c" class="help-txt-hidden"
                                                            label="Search Term 1" variant="label-hidden"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            disabled="{!v.PendingApproval}"
                                                            value="{!v.requestFormHeaderObj.SupplierSearchTermLocal__c}"
                                                            maxlength="20" />
                                                        <!--<lightning:inputField
                                                            fieldName="SupplierSearchTermLocal__c"
                                                            variant="label-hidden" required="true"  value="{!v.requestFormHeaderObj.SupplierSearchTermLocal__c}" />-->
                                                    </aura:if>
                                                </div>
                                                <!-- Search Term 2(EN) -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <p style="text-align: left; padding-left: 62px;">
                                                        Search Term 2(EN)
                                                    </p>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6"
                                                    style="padding-bottom: 16px; margin-top: 10px;">
                                                    <aura:if isTrue="{!v.recordTypeNameCustomer == True}">
                                                        <lightning:input name="CustomerSearchTermEN__c" class="help-txt-hidden"
                                                            label="Search Term 2(EN)" variant="label-hidden"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            disabled="{!v.PendingApproval}"
                                                            value="{!v.requestFormHeaderObj.CustomerSearchTermEN__c}"
                                                            maxlength="20" />
                                                        <!--<lightning:inputField
                                                            fieldName="CustomerSearchTermEN__c"
                                                            variant="label-hidden"  value="{!v.requestFormHeaderObj.CustomerSearchTermEN__c}" />-->
                                                    </aura:if>
                                                    <aura:if isTrue="{!v.recordTypeNameCustomer == False}">
                                                        <lightning:input name="SupplierSearchTermEN__c" class="help-txt-hidden"
                                                            label="Search Term 2(EN)" variant="label-hidden"
                                                            onchange="{!c.handleAddJsonHeader}"
                                                            disabled="{!v.PendingApproval}"
                                                            value="{!v.requestFormHeaderObj.SupplierSearchTermEN__c}"
                                                            maxlength="20" />
                                                        <!--<lightning:inputField
                                                            fieldName="SupplierSearchTermEN__c"
                                                            variant="label-hidden" value="{!v.requestFormHeaderObj.SupplierSearchTermEN__c}" />-->
                                                    </aura:if>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Address Section -->
                                <div class="slds-col slds-size_1-of-1">
                                    <div class="slds-section slds-is-open" aura:id="addressSection">
                                        <h3 class="slds-section__title">
                                            <button type="button" aria-controls="addressDetail" aria-expanded="true"
                                                class="slds-button slds-section__title-action"
                                                data-auraid="addressSection" onclick="{!c.handleToggleSection}">
                                                <lightning:icon iconName="utility:switch" alternativeText="switch"
                                                    title="" size="x-small"
                                                    class="slds-section__title-action-icon slds-button__icon slds-button__icon_left" />
                                                <span class="slds-truncate" title="Address">Address</span>
                                            </button>
                                        </h3>
                                        <div class="slds-section__content" id="addressDetail">
                                            <div class="slds-grid slds-wrap slds-var-p-around_large">
                                                <!-- Buliding Codo/Room/Floor -->
                                                <div
                                                    class="slds-col slds-size_4-of-12 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        Building Code/Room/Floor
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_2-of-12"
                                                    style="padding-right: 12px; padding-bottom: 16px; text-align: start;">
                                                    <lightning:input name="BuildingCodeLocal__c" label="Building Code" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.BuildingCodeLocal__c}" />
                                                    <!-- <lightning:inputField fieldName="BuildingCodeLocal__c" variant="label-hidden" value="{!v.requestFormHeaderObj.BuildingCodeLocal__c}" /> -->
                                                </div>
                                                <div class="slds-col slds-size_2-of-12" style="text-align: start;">
                                                    <lightning:input name="RoomLocal__c" label="Room" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.RoomLocal__c}" />
                                                    <!-- <lightning:inputField fieldName="RoomLocal__c" variant="label-hidden" value="{!v.requestFormHeaderObj.RoomLocal__c}" /> -->
                                                </div>
                                                <div class="slds-col slds-size_2-of-12"
                                                    style="padding-left: 12px; padding-bottom: 16px; text-align: start;">
                                                    <lightning:input name="FloorLocal__c" label="Floor" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.FloorLocal__c}" />
                                                    <!-- <lightning:inputField fieldName="FloorLocal__c" variant="label-hidden" value="{!v.requestFormHeaderObj.FloorLocal__c}" /> -->
                                                </div>
                                                <!-- c/o -->
                                                <div
                                                    class="slds-col slds-size_4-of-12 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        c/o
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_6-of-12" style="padding-bottom: 16px;">
                                                    <lightning:input name="CoName__c" label="CoName" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.CoName__c}" />
                                                    <!-- <lightning:inputField fieldName="CoName__c" variant="label-hidden"  value="{!v.requestFormHeaderObj.CoName__c}" /> -->
                                                </div>
                                                <!-- row 1* -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        Street 2
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6" style="padding-bottom: 16px;">
                                                    <lightning:input name="Street2Local__c" label="Street2 (Local)" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}" maxlength="40"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.Street2Local__c}" />
                                                    <!-- <lightning:inputField fieldName="Street2Local__c"
                                                        variant="label-hidden"  value="{!v.requestFormHeaderObj.Street2Local__c}" /> -->

                                                </div>
                                                <!-- row 2* -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        Street 3
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6" style="padding-bottom: 16px;">
                                                    <lightning:input name="Street3Local__c" label="Street3 (Local)" maxlength="40" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.Street3Local__c}" />
                                                    <!-- <lightning:inputField fieldName="Street3Local__c"
                                                        variant="label-hidden"  value="{!v.requestFormHeaderObj.Street3Local__c}" /> -->

                                                </div>
                                                <!-- Street/House number* -->
                                                <div
                                                    class="slds-col slds-size_4-of-12 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 50px; display: flex;">
                                                        <p style="color: red;">* &nbsp;</p>Street/House number
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_4-of-12"
                                                    style="padding-bottom: 16px; text-align: start;">
                                                    <lightning:input name="StreetLocal__c" label="Street (Local)" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}" maxlength="60"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.StreetLocal__c}" />
                                                    <!-- <lightning:inputField fieldName="StreetLocal__c"
                                                        variant="label-hidden" required="true"  value="{!v.requestFormHeaderObj.StreetLocal__c}" /> -->

                                                </div>
                                                <div class="slds-col slds-size_2-of-12"
                                                    style="padding-left: 10px; padding-bottom: 16px;">
                                                    <lightning:input name="HousenoLocal__c" label="House number" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}"
                                                        disabled="{!v.PendingApproval}"
                                                        value="{!v.requestFormHeaderObj.HousenoLocal__c}" />
                                                    <!-- <lightning:inputField fieldName="HousenoLocal__c"
                                                        variant="label-hidden"  value="{!v.requestFormHeaderObj.HousenoLocal__c}" /> -->

                                                </div>

                                                <!-- row 3* -->
                                                <div
                                                    class="slds-col slds-size_2-of-6 slds-p-horizontal_x-small slds-p-top_medium">
                                                    <div style="text-align: left; padding-left: 62px; display: flex;">
                                                        Street 4
                                                    </div>
                                                </div>
                                                <div class="slds-col slds-size_3-of-6" style="padding-bottom: 16px;">
                                                    <lightning:input name="Street4Local__c" label="Street4 (Local)" class="help-txt-hidden"
                                                        variant="label-hidden" onchange="{!c.handleAddJsonHeader}" maxlength="40"
