                                                        <!--<lightning:helptext content="Scores are automatically based on information provided by the customer."></lightning:helptext>-->

                                                        <span style="position:relative;" class="padding-help">
                                                            <span class="slds-form-element">
                                                                <div class="slds-form-element__icon">
    
                                                                    <button
                                                                        class="slds-button slds-button_icon slds-button slds-button_icon"
