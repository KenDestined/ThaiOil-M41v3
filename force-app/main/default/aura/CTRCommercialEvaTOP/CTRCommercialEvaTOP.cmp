                                                    <lightning:inputField aura:id="TheEfficiencyLevelOfFacility__c"
                                                        fieldName="TheEfficiencyLevelOfFacility__c"
                                                        variant="label-hidden" disabled="{!v.IsDisable}"
                                                        onchange="{!c.handlePercentCalculate}" required="true" />
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
