<aura:component controller="CTRInitialCustomerTOP"
    implements="lightning:actionOverride,force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
    access="global">

    <aura:handler name="init" action="{!c.doInit}" value="{!this}" />
    <aura:attribute name="IsDisable" type="Boolean" default="true" />
    <aura:attribute name="IsDomesticDisabled" type="Boolean" default="true" />
    <aura:attribute name="IsDomesticRequired" type="Boolean" default="false" />
    <aura:attribute name="IsInterDisabled" type="Boolean" default="true" />
    <aura:attribute name="IsInterRequired" type="Boolean" default="false" />
    <aura:attribute name="isTX" type="Boolean" default="false" />
    <aura:attribute name="BUInfo" type="CTRBuProfile__mdt" />
    <aura:attribute name="LogInUserId" type="String" />
    <aura:attribute name="Country" type="String" />
    <aura:attribute name="SalesOrg" type="String" />
    <aura:attribute name="GoogleLink" type="String" />
    <aura:attribute name="IsShowedButton" type="Boolean" default="true" />
    <aura:attribute name="IsShowPage" type="Boolean" default="false" />
    <aura:attribute name="NotShowText" type="String" />
    <aura:attribute name="tooltip1" type="boolean" default="false" />
    <aura:attribute name="tooltip2" type="boolean" default="false" />
    <aura:attribute name="tooltip3" type="boolean" default="false" />
    <aura:attribute name="tooltip4" type="boolean" default="false" />
    <aura:attribute name="tooltip5" type="boolean" default="false" />
    <aura:attribute name="tooltip6" type="boolean" default="false" />
    <aura:attribute name="tooltip7" type="boolean" default="false" />

    <aura:renderIf isTrue="{!!v.IsShowPage}">
        <br /><br />
        <center>
            <p>{!v.NotShowText}</p>
        </center>
        <br /><br />
    </aura:renderIf>
    <aura:renderIf isTrue="{!v.IsShowPage}">
        <div class="slds-float_right">
            <aura:renderIf isTrue="{!v.IsShowedButton}">
                <lightning:button variant="brand-outline" label="Commercial Evaluation" title="Commercial Evaluation"
                    onclick="{!c.CommercialhandleClick }" />
            </aura:renderIf>
        </div><br /> <br />
        <!--<lightning:button variant="brand-outline" label="Commercial Evaluation" title="Commercial Evaluation" onclick="{!c.CommercialhandleClick }" />-->
        <lightning:layoutItem padding="around-small">
            <lightning:recordEditForm aura:id="recordEditForm" objectApiName="CTRRequestFormItem__c"
                recordId="{!v.recordId}" onsubmit="{!c.handleSubmit}" onsuccess="{!c.handleSuccess}">

                <div class="slds-section slds-is-open animate" aura:id="Commercial">
                    <h3 class="slds-section__title">
                        <button aria-controls="CommercialList" class="slds-button slds-section__title-action"
                            onclick="{!c.toggleSection}" data-auraId="Commercial">
                            <lightning:icon iconName="utility:switch" size="x-small"
                                class="slds-section__title-action-icon slds-button__icon_left"
                                alternativeText="button icon" />
                            <span class="slds-truncate" title="Counterparty Information">Pre-Screen-Commercial
                                Evaluation</span>
                        </button>
                    </h3>
                    <div class="slds-section__content" id="CommercialList">
                        <div class="slds-grid slds-wrap">
                            <!-- <div class="slds-col slds-size_1-of-2 slds-var-p-around_medium"> -->
                            <aura:if isTrue="{!!v.isTX}">
                                <aura:if isTrue="{!and(v.SalesOrg == '1100',v.Country == 'TH')}">
                                    <div class="slds-col slds-size_1-of-1 slds-var-p-horizontal_medium">
                                        <!-- <label>1. Number of registration issues by Ministry of Energy</label>
                                            <lightning:inputField aura:id="NumberRegistrationMinistryEnergy__c" fieldName="NumberRegistrationMinistryEnergy__c" variant="label-hidden" disabled="{!v.IsDisable}" onchange="{!c.handlePercentCalculate}"/> -->
                                        <div class="slds-form">
                                            <div class="slds-form-element slds-var-p-around_medium">

                                                <label class="slds-form-element__label">
                                                    <span style="color:red; font-style: italic;">* </span>
                                                    Do you have oil trader registration letter (N.P. 104 or N.P.105 or
                                                    N.P.106) which issued by Ministry of Energy?
                                                    <br />(ใบทะเบียนเป็นผู้ค้าน้ำมันตามมาตราต่างๆ
                                                    ที่ออกโดยกรมธุรกิจพลังงาน)
                                                    <!--<lightning:helptext content="Scores are automatically based on information provided by the customer."></lightning:helptext>-->
                                                    <span style="position:relative;" class="padding-help">
                                                        <span class="slds-form-element">
                                                            <div class="slds-form-element__icon">

                                                                <button
                                                                    class="slds-button slds-button_icon slds-button slds-button_icon"
                                                                    aria-describedby="help1" title="Help"
                                                                    onmouseover="{!c.showToolTip1}"
                                                                    onmouseout="{!c.HideToolTip1}">
                                                                    <lightning:icon iconName="utility:info"
                                                                        size="xx-small" />
                                                                    <span class="slds-assistive-text">Help</span>
                                                                </button>
                                                            </div>
                                                        </span>
                                                        <aura:if isTrue="{!v.tooltip1}">
                                                            <div class="slds-popover slds-popover_tooltip slds-nubbin_bottom-left tooltipFormat"
                                                                role="tooltip" id="help1">
                                                                <div class="slds-popover__body popup_Class">Scores are
                                                                    auto-based on information provided by the
                                                                    customer.<br />
                                                                    1 = There are a number of registration issues with
                                                                    the Ministry of Energy.<br />
                                                                    4 = There are no registration issues with the
                                                                    Ministry of Energy.</div>
                                                            </div>

                                                        </aura:if>
                                                    </span>
                                                </label>
                                                <div class="slds-form-element__control slds-var-p-top_xxx-small"
                                                    style="padding-left: 0% !important;">
                                                    <lightning:inputField aura:id="NumberRegistrationMinistryEnergy__c"
                                                        fieldName="NumberRegistrationMinistryEnergy__c"
                                                        variant="label-hidden" disabled="{!v.IsDisable}"
                                                        onchange="{!c.handlePercentCalculate}" required="true" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </aura:if>
                            </aura:if>
                            <div class="slds-col slds-size_1-of-1 slds-var-p-horizontal_medium">
                                <!-- <label>2. Level of reputation in the market and credibility of a company</label>
                                        <lightning:inputField aura:id="Thelevelofreputation__c" fieldName="Thelevelofreputation__c" variant="label-hidden" disabled="{!v.IsDisable}" onchange="{!c.handlePercentCalculate}"/> -->
                                <div class="slds-form">
                                    <div class="slds-form-element">
                                        <div class="slds-grid">
                                            <div
                                                class="slds-col slds-size_1-of-2 slds-var-p-around_medium responsive-block">
                                                <div>
                                                    <label class="slds-form-element__label">
                                                        <span style="color:red; font-style: italic;">* </span> Level of
                                                        reputation in the market and credibility of a company
                                                        <!--<lightning:helptext content="Scores are automatically determined based on customer-provided 'Business Type' and 'Legal Entity' information, specifically for options that meet the criteria."></lightning:helptext>-->
                                                        <span style="position:relative;" class="padding-help">
                                                            <span class="slds-form-element">
                                                                <div class="slds-form-element__icon">
    
                                                                    <button
                                                                        class="slds-button slds-button_icon slds-button slds-button_icon"
                                                                        aria-describedby="help2" title="Help"
                                                                        onmouseover="{!c.showToolTip2}"
                                                                        onmouseout="{!c.HideToolTip2}">
                                                                        <lightning:icon iconName="utility:info"
                                                                            size="xx-small" />
                                                                        <span class="slds-assistive-text">Help</span>
                                                                    </button>
                                                                </div>
                                                            </span>
                                                            <aura:if isTrue="{!v.tooltip2}">
                                                                <div class="slds-popover slds-popover_tooltip slds-nubbin_bottom-left tooltipFormat"
                                                                    role="tooltip" id="reputation">
                                                                    <div class="slds-popover__body popup_Class">Scores are
                                                                        auto-based on customer's 'Business Type' and 'Legal
                                                                        Entity' for eligible options.<br />
                                                                        1 = Other-Other, End User, Supplier w/ Private
                                                                        individual or Sole proprietorship<br />
                                                                        2 = Trading Firm, End User, Supplier w/ Private
                                                                        limited company<br />
                                                                        3 = National Oil, Trading Firm, Manufacturer,
                                                                        Refinery w/ Subsidiary company of public limited
                                                                        company<br />
                                                                        4 = National Oil, Trading Firm, Manufacturer,
                                                                        Refinery w/ Public limited company<br />
                                                                        Other matches of 'Business Type' and 'Legal Entity'
                                                                        are scored by the request owner.</div>
                                                                </div>
    
                                                            </aura:if>
                                                        </span>
                                                    </label>
                                                </div>
                                                <div class="slds-form-element__control slds-var-p-top_xxx-small responsive-col"
                                                    style="padding-left: 0% !important;">
                                                    <lightning:inputField aura:id="Thelevelofreputation__c"
                                                        fieldName="Thelevelofreputation__c" variant="label-hidden"
                                                        disabled="{!v.IsDisable}" onchange="{!c.handlePercentCalculate}"
                                                        required="true" />
                                                </div>

                                            </div>
                                            <div
