<apex:page controller="CTRDisclaimerPDFController" applyHtmlTag="false" showHeader="false" renderAs="PDF" action="{!renderPage}" title="Disclaimer">

<html>
    <head>
        <style>
            * {
                box-sizing: border-box;
            }

            html,
            body {
                line-height: 16px;
            }
            table { 
            font-size: 9px; 
            table-layout: fixed; 
            } 
  
            td { 
            text-align: left;
            padding: 0.5px;
            /* border: 1px solid gray; */
            } 
            p {
                margin: 0px;
                padding: 0px;
            }

            .subject-space {
                margin-top: 15px;
            }

            body {
                font-family: Arial Unicode MS;
                font-size: 9px;
                color: #3f3d3d;
            }

            font {
                font-family: Arial Unicode MS !important;
            }

            .inline {
                display: inline-block;
            }

            .inline div {
                display: inline-block;
            }

            @page {
                size: A4;
                margin: 1cm 0.75cm 1.25cm 0.75cm;
                font-family: 'Arial Unicode MS';
                /* font-size: 9.5pt; */
                /* background-color: #fff; */
                /* border: 1px solid; */
                padding: 0;
                @bottom-right {
                    /* content: "Page " counter(page); */
                    content: element(footer);
                }
            }
            .footer {
                /* display: flex;
                position: fixed; */
                display: block;
                position: running(footer);
                text-align: justify;
                font-size: 12px;
                color: #000000;
            }
                .pagenumber:before {
                    content: counter(page);
                    float: right;
                }
                .pagecount:before {
                    content: counter(pages);
                }
            .page-break {
            display:block;
            page-break-after:always;
            }
            .section-header {
                /* background-color: grey; */
                background-color: #D3D3D3;
                color: #000000;
                font-weight: bold;
                font-size: 10px;
            }
            .fontDisclaimer{
                font-weight:bold;
                color: #000000;
                text-align: center;
                margin-left: 30px;
                font-size: 15px;
            }
            .left-padded {
                padding-left: 25px;
            }
            .left-margin {
                margin-left: 5px;
            }
            .topic-section {
                color: #000000;
                font-weight: bold;
                font-size: 9px;
                padding-left: 25px;
            }
            .section-inline {
                color: #000000;
                font-weight: bold;
                font-size: 9px;
            }
            .long-link {
                display: inline-block;
                width: 500px;
                max-width: 500px;
                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
            }
            
            .checkbox {
                display: inline-block;
                width: 5px;
                height: 5px;
                background: gray;
                color: white;
                line-height: 0;
                padding: 5px;
                border: 1px solid gray;
                vertical-align: middle;

            }
            .checkboxaddress {
                display: inline-block;
                width: 5px;
                height: 5px;
                background: gray;
                color: white;
                line-height: 3px;
                padding: 5px;
        		margin: 5px;
                border: 1px solid gray;
            }
            .endorseheader {
                margin-top: 50px;
                text-align: right;
                /* margin-right: 200px; */
            }
            .endorsement {
                text-align: right;
                margin-right: 50px;
            }
            .signature {
                margin-top: 10px;
                margin-right: 50px;
                display: inline-block;
            }
            .container {
                display: flex;
                justify-content: center;
                align-items: center;
                width: 100%;
                }
                .image-container {
                    display: inline-block;
                    /* justify-content: space-around; */
                    flex-direction: row;
                    width: 100%;
                    margin: 0 10px; 
                    text-align: center;
                }
                .image-item{
                    /* display: flex;  */
                    display: inline-block;
                    /* flex-direction: column; */
                }
                img {
                    width: 100px;
                    height: auto; 
                    margin: 0 50px;
                }
        </style>
        
    </head>
    <body>
        <!-- Footer -->
        <div class="footer">
            <span>Generated on {!formattedDateTime}</span>
            <span style="float: right;">Page <span class="pagenumber"/></span>
        </div>
        
        <!-- Page 1 -->
        <div>
            <!-- {!selectedLanguage}, {!recordId} -->
            <img src="{!$Resource.Logo}" style="display: block; width: 25%; margin-left: auto; margin-right: auto;"/>
            <br />
            <apex:outputPanel rendered="{!selectedLanguage == 'Thai'}">
                <div class="fontDisclaimer">
                     เอกสารข้อสงวนสิทธิ์
                </div>
                <br />
                <section>
                    <div class="section-header">
                        ข้อกำหนดและเงื่อนไข
                    </div>
                    <div class="section-content" style="width: 100%">
                            <ol>
                                <li>บริษัทคู่ค้ารับรองและรับประกันว่าข้อมูลที่ให้นี้เป็นข้อมูลที่ได้ให้โดยถูกต้องในนามของบริษัทคู่ค้า ทั้งยังเป็นข้อมูลที่ถูกต้องและแท้จริง
                                    ณ เวลาลงทะเบียน</li>
                                <li>บริษัท ไทยออยล์ จำกัด (มหาชน) ขอสงวนสิทธิแต่เพียงผู้เดียวที่จะยกเลิกหรือเพิกถอนการลงทะเบียนคู่ค้า หากพบว่าบริษัทคู่ค้าไม่มีคุณสมบัติ
                                    สำหรับการลงทะเบียน หรือข้อมูลที่ได้ให้โดยบริษัทคู่ค้าเป็นข้อมูลที่ไม่ถูกต้อง หรือเป็นข้อมูลเท็จ ทั้งนี้ ภายใต้เงื่อนไขว่าการยกเลิกหรือเพิกถอน
                                    การลงทะเบียนจะไม่กระทบต่อสิทธิและหน้าที่ของทั้งสองฝ่ายตามธุรกรรมที่มีอยู่</li>
                                <li>บริษัทคู่ค้าอาจถูกร้องขอข้อมูลเพิ่มเติมระหว่างการพิจารณา</li>
                                <li>กลุ่มไทยออยล์มีความมุ่งมั่นต่อการเคารพหลักสิทธิมนุษยชนตามที่ระบุในกฎบัตรระหว่างประเทศว่าด้วยสิทธิมนุษยชนในทุกการดำเนินธุรกิจตลอด
                                    ห่วงโซ่คุณค่า กลุ่มไทยออยล์จะดำเนินการที่จำเป็นเพื่อให้มั่นใจว่าพนักงาน กิจกรรม ผลิตภัณฑ์ หรือบริการ ตลอดจนคู่ค้าจะหลีกเลี่ยงการเข้าไปมี
                                    ส่วนเกี่ยวข้องกับการละเมิดสิทธิมนุษยชน</li>
                            </ol>
                        <br />
                            <div class="left-padded">
                                เพื่อให้บรรลุเป้าหมายดังกล่าว กลุ่มไทยออยล์ได้ดำเนินกระบวนการตรวจสอบตามหลักการชี้แนะว่าด้วยธุรกิจและสิทธิมนุษยชนแห่งสหประชาชาติ
                                คู่ค้าของไทยออยล์ต้องปฏิบัติตามมาตรฐานว่าด้วยสิทธิมนุษยชนระหว่างประเทศ และต้องปฏิบัติตามนโยบายด้านธุรกิจและสิทธิมนุษยชนสำหรับคู่ค้า
                                ของกลุ่มไทยออยล์ ตลอดจนจะขยายความคาดหวังดังกล่าวนี้ไปยังคู่ค้าอื่นของตนด้วย
                            </div>
                    </div>
                </section>
            </apex:outputPanel>

            <apex:outputPanel rendered="{!selectedLanguage == 'Vietnamese'}">
                <div class="fontDisclaimer">
                    Tài liệu Từ chối trách nhiệm
                </div>
                <br />
                <section>
                    <div class="section-header">
                        Các điều khoản và điều kiện
                    </div>
                    <div class="section-content" style="width: 100%">
                            <ol>
                                <li>Công ty đối tác xác nhận và đảm bảo rằng thông tin đã cung cấp là chính xác, nhân danh công ty đối tác, và đúng sự thật tại thời điểm đăng ký.</li>
                                <li>Công ty Trách nhiệm hữu hạn Thai Oil (Đại chúng) có toàn quyền hủy bỏ hoặc thu hồi việc đăng ký đối tác, nếu phát hiện công ty
                                    đối tác không đủ điều kiện đăng ký, hoặc thông tin mà công ty đối tác cung cấp là không chính xác hoặc sai sự thật, theo điều kiện
                                    rằng việc hủy bỏ hoặc thu hồi việc đăng ký sẽ không ảnh hưởng đến quyền và nghĩa vụ của hai bên theo giao dịch hiện tại.</li>
                                <li>Các công ty đối tác có thể được yêu cầu cung cấp thêm thông tin trong quá trình xem xét.</li>
                                <li>Tập đoàn Thai Oil cam kết tôn trọng các nguyên tắc nhân quyền được quy định trong Hiến chương Quốc tế về Nhân quyền trong mọi
                                    hoạt động kinh doanh, xuyên suốt chuỗi giá trị. Tập đoàn Thai Oil sẽ thực hiện các việc cần thiết để đảm bảo rằng các nhân viên, hoạt
                                    động, sản phẩm hoặc dịch vụ, và đối tác sẽ tránh liên quan đến những hành vi vi phạm nhân quyền.</li>
                            </ol>
                        <br />
                            <div class="left-padded">
                                Để đạt được mục tiêu này, Tập đoàn Thai Oil đã thực hiện quy trình kiểm tra theo Nguyên tắc Hướng dẫn về Kinh doanh và Nhân quyền
                                của Liên hợp quốc. Các đối tác thương mại của Thai Oil phải tuân thủ các tiêu chuẩn nhân quyền quốc tế, và phải tuân thủ Chính sách về
                                Kinh doanh và Nhân quyền dành cho các đối tác của Tập đoàn Thai Oil, và cũng sẽ mở rộng những kỳ vọng này tới các đối tác khác
                                của họ.
                            </div>
                    </div>
                </section>
            </apex:outputPanel>

            <apex:outputPanel rendered="{!selectedLanguage == 'Bahasa'}">
                <div class="fontDisclaimer">
                    Surat Penafian
                </div>
                <br />
                <section>
                    <div class="section-header">
                        Syarat dan Ketentuan
                    </div>
                    <div class="section-content" style="width: 100%">
                            <ol>
                                <li>Perusahaan mitra menyatakan dan menjamin bahwa informasi yang diberikan adalah akurat dan akurat atas nama perusahaan
                                    mitra. Juga merupakan informasi yang benar dan tepat pada saat pendaftaran.</li>
                                <li>Thai Oil Public Company Limited memiliki hak tunggal untuk membatalkan atau mencabut pendaftaran mitra , jika ternyata
                                    perusahaan mitra tidak memenuhi syarat untuk mendaftar atau informasi yang diberikan oleh perusahaan mitra tidak benar, atau
                                    keterangan palsu. Dengan syarat pembatalan atau pencabutan pendaftaran tidak akan mempengaruhi hak dan kewajiban kedua
                                    belah pihak sesuai transaksi yang ada.</li>
                                <li>Perusahaan mitra mungkin diminta untuk memberikan informasi tambahan dengan pertimbangan.</li>
                                <li>Thai oil Group berkomitmen untuk menghormati prinsip-prinsip hak asasi manusia sebagaimana diatur dalam Piagam Internasional
                                    Hak Asasi Manusia dalam semua aspek operasi bisnisnya. Thai oil Group akan mengambil langkah-langkah yang diperlukan untuk
                                    memastikan bahwa karyawan, aktivitas, produk atau layanan, dan perusahaan mitra akan menghindari keterlibatan dalam
                                    pelanggaran hak asasi manusia</li>
                            </ol>
                        <br />
                            <div class="left-padded">
                                Untuk mencapai tujuan ini, Thai oil Group telah melakukan proses audit sesuai dengan Prinsip Panduan PBB tentang bisnis dan Hak
                                Asasi Manusia. Pemasok Thai oil harus mematuhi standard hak asasi manusia internasional dan harus mematuhi kebijakan bisnis dan
                                hak asasi manusia bagi perusahaan mitra Thai oil Group, Mereka harus tunduk pada aturan tersebut, serta akan memperluas
                                ekspektasi ini kepada mitra dagang mereka yang lain.
                            </div>
                    </div>
                </section>
            </apex:outputPanel>

            <br />
            <section>
                <div class="section-header">
                    Company Information
                </div>
                <div class="section-content">
                    <table style="width: 100%">
                        <tbody>
                            <tr>
                                <td>
                                    <span class="topic-section">Company Name (English) : </span>
                                    <span class="left-margin">{!accountObj.CompanyNameENF__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Company Name (Other Languages) : </span> 
                                    <span class="left-margin">{!accountObj.CompanyNameLocal__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Specify Languages of Company Name : </span> 
                                    <span class="left-margin">{!accountObj.SpecifyLanguagesOfCompanyName__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Former Name of Company (If any) : </span> 
                                    <span class="left-margin">{!accountObj.FormerNameofCompany__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Country : </span> 
                                    <span class="left-margin">
                                        <!-- {!accountObj.Country__r.Code__c} - {!accountObj.Country__r.Description__c} -->
                                        {!IF(
                                            NOT(ISBLANK(accountObj.Country__r.Code__c)) && NOT(ISBLANK(accountObj.Country__r.Description__c)),
                                            (accountObj.Country__r.Code__c) + ' - ' + (accountObj.Country__r.Description__c),
                                            ''
                                        )}
                                    </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Customer Type : </span> 
                                    <span class="left-margin">{!accountObj.CustomerType__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Supplier Type : </span> 
                                    <span class="left-margin">{!accountObj.SupplierType__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Type of Business : </span> 
                                    <span class="left-margin">{!accountObj.TypeOfBusinessCommercialEvaluation__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Other Business Type (Please specify) : </span> 
                                    <span class="left-margin">{!accountObj.TypeOfBusinessOthers__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Legal Entity : </span> 
                                    <span class="left-margin">{!accountObj.LegalEntity__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Other Legal Entity (Please specify) : </span> 
                                    <span class="left-margin">{!accountObj.LegalEntityOthers__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Incorporation Date : </span> 
                                    <span class="left-margin">{!formattedIncorporationDate}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Years of Experience in business? : </span> 
                                    <span class="left-margin">{!accountObj.YearsofExperience__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">TAX Number : </span> 
                                    <span class="left-margin">{!accountObj.TaxCode1__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Company Website : </span> 
                                    <span class="left-margin">{!accountObj.CompanyWebsite__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">CEO/President/Managing Director First Name : </span> 
                                    <span class="left-margin">{!accountObj.CEOFirstName__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Last Name : </span> 
                                    <span class="left-margin">{!accountObj.CEOLastName__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Immediate Parent Company : </span> 
                                    <span class="left-margin">{!accountObj.ImmidiateParentCompany__c}</span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </section>
            <br />
            <section style="margin-bottom: 10px">
                <div class="section-header">
                    List of Shareholders who hold an interest in the company more than 10%
                </div>
                <table style="width: 100%">
                    <tbody>
                        <apex:repeat var="item" value="{!shareholderList}">
                            <tr>
                                <td>
                                    <strong class="topic-section">Name : </strong>
                                    <span class="left-margin">{!item.Name}</span>
                                </td>
                                <td>
                                    <strong class="section-inline">% of Shareholders </strong>
                                    <span class="left-margin">{!item.of_Shareholder__c}</span>
                                </td>
                            </tr>
                        </apex:repeat>
                    </tbody>
                </table>
            </section>
            <br />
            <section>
                <div class="section-header">
                    Registered Address
                </div>
                <div class="section-content">
                    <table style="width: 100%">
                        <tbody>
                            <tr>
                                <td>
                                    <span class="topic-section">Building Code : </span> 
                                    <span class="left-margin">{!accountObj.BuildingCodeLocal__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Room : </span> 
                                    <span class="left-margin">{!accountObj.RoomLocal__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Floor : </span> 
                                    <span class="left-margin">{!accountObj.FloorLocal__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">c/o : </span> 
                                    <span class="left-margin">{!accountObj.CoName__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">House no. : </span> 
                                    <span class="left-margin">{!accountObj.HousenoLocal__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Street : </span> 
                                    <span class="left-margin">{!accountObj.StreetLocal__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Sub District : </span> 
                                    <span class="left-margin">{!accountObj.SubDistrictLocalText__c}</span>
                                </td>
                                <td>
                                    <span class="section-inline">District : </span> 
                                    <span class="left-margin">{!accountObj.DistrictLocalText__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">City/State : </span> 
                                    <span class="left-margin">{!accountObj.CityStateLocal__r.Name}</span>
                                </td>
                                <td>
                                    <span class="section-inline">Postal Code : </span> 
                                    <span class="left-margin">{!accountObj.PostalCodeLocalText__c}</span>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <span class="topic-section">Location in Google Maps (Link) : </span> 
                                    <a href="{!accountObj.LocationInGoogleMapsLinks__c}" class="long-link">
                                        {!accountObj.LocationInGoogleMapsLinks__c}
                                    </a>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <span class="topic-section">Location in Google Maps (Latitude, Longtitude) : </span> 
                                    <!-- <span class="left-margin">{!accountObj.Location__Latitude__s} , {!accountObj.Location__Longitude__s}</span> -->
                                    <span class="left-margin">
                                        {!IF(
                                            NOT(ISBLANK(accountObj.LocationInGoogleMapsGeolocation__Latitude__s)) && NOT(ISBLANK(accountObj.LocationInGoogleMapsGeolocation__Longitude__s)),
                                            TEXT(accountObj.LocationInGoogleMapsGeolocation__Latitude__s) + ', ' + TEXT(accountObj.LocationInGoogleMapsGeolocation__Longitude__s),
                                            ''
                                        )}
                                    </span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </section>
        </div>

        <!-- Page 2 -->
        <div style="page-break-after: always"/>
        <div>
            <img src="{!$Resource.Logo}" style="display: block; width: 25%; margin-left: auto; margin-right: auto;"/>
            <br />
            <div class="fontDisclaimer">
                <apex:outputPanel rendered="{!selectedLanguage == 'Thai'}">
                    <!-- Thai language content -->
                    เอกสารข้อสงวนสิทธิ์
                </apex:outputPanel>
                <apex:outputPanel rendered="{!selectedLanguage == 'Vietnamese'}">
                    <!-- Vietnamese language content -->
                    Tài liệu Từ chối trách nhiệm
                </apex:outputPanel>
                <apex:outputPanel rendered="{!selectedLanguage == 'Bahasa'}">
                    <!-- Bahasa language content -->
                    Surat Penafian
                </apex:outputPanel>
            </div>
            <br />
            <section>
                <div class="section-header">
                    Mailing Address
                </div>
                <div class="section-content">
                    <table style="width: 100%">
                        <tbody>
                            <tr>
                                <td>
